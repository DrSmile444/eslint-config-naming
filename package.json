{
  "name": "@drsmile444/eslint-config-naming",
  "version": "1.3.0",
  "description": "Opinionated ESLint naming convention config for TypeScript projects",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./legacy": {
      "import": "./dist/legacy.mjs",
      "require": "./dist/legacy.cjs",
      "default": "./dist/legacy.mjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npx tsx ./src/main.ts",
    "lint": "cross-env TIMING=1 npx eslint",
    "lint:fix": "npm run lint -- --fix",
    "lint:debug": "npm run lint -- --debug",
    "build": "tsup",
    "format:md": "npx prettier --write \"**/*.md\"",
    "typecheck": "npx tsc --noEmit",
    "test": "npx vitest run",
    "bump-version": "npx tsx ./scripts/bump-version.script.ts && npm audit fix",
    "parse-llm-response": "npx tsx ./scripts/split-plain-code.ts",
    "format": "npx prettier . --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "typescript",
    "naming-convention"
  ],
  "author": "Dmytro Vakulenko",
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0 || ^9.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@dword-design/eslint-plugin-import-alias": "^6.0.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^25.0.3",
    "@vitest/eslint-plugin": "^1.5.2",
    "cross-env": "^10.1.0",
    "deepmerge": "^4.3.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-project-structure": "^3.14.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "json5": "^2.2.3",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "type-fest": "^5.3.1",
    "typescript-eslint": "^8.50.0",
    "unplugin-swc": "^1.5.9",
    "vite-tsconfig-paths": "^6.0.3",
    "vitepress": "^2.0.0-alpha.15",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "eslint-config-airbnb-base": {
      "eslint": "^9"
    }
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git@github-personal:DrSmile444/typescript-boilerplate.git"
  }
}
